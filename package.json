{
  "name": "kusako-discord",
  "version": "2.0.0",
  "description": "discord bot with AI integration, function calling, and additional configuration for power users",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/bot/index.js",
    "dev": "tsx watch src/bot/index.ts",
    "deploy": "tsx src/bot/deploy-commands.ts",
    "deploy:global": "tsx src/bot/deploy-commands.ts --global",
    "deploy:clear": "tsx src/bot/deploy-commands.ts --clear",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conjureme/kusako-discord.git"
  },
  "keywords": [
    "discord",
    "bot",
    "ai",
    "typescript"
  ],
  "author": "tyler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/conjureme/kusako-discord/issues"
  },
  "homepage": "https://github.com/conjureme/kusako-discord#readme",
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.18.0",
    "discord.js": "^14.20.0",
    "dotenv": "^16.5.0",
    "prism-media": "^1.3.5"
  },
  "devDependencies": {
    "@types/node": "^24.0.4",
    "prettier": "^3.5.3",
    "tsx": "^4.20.3"
  }
}
